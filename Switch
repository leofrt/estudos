<?php
     
     $d = isset($_GET["ds"])?$_GET["ds"]:0;
     
     switch ($d) {
         case 2:
         case 3:
         case 4:
         case 5:
         case 6:
             echo "Dia de estudar";
             break;
         case 7:
         case 8:
             echo "final de semana";
             break;

    default:
        echo "dia invalido";
}
